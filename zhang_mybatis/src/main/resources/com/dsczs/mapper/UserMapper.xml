<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.dsczs.mapper.UserMapper">    <insert id="insertBatch">        INSERT INTO `user` (`name`) VALUES        <foreach collection="list" item="item" index="index" separator=",">            (#{item.name})        </foreach>    </insert>    <select id="selectList" resultType="com.dsczs.model.User">        SELECT * FROM `user`;    </select></mapper>